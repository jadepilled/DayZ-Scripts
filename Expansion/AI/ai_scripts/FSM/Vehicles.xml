		<state name="Sitting">
			<variables />
			<editor_data>
				<position x="1239.82498598693" y="781.144645880103" />
				<size width="150" height="40" />
			</editor_data>
			<event_entry />
			<event_exit />
			<event_update>
				return EXIT;
