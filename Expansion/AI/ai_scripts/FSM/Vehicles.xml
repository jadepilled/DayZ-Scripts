			<to_state />
		</transition>
		<transition>
			<editor_data>
				<position_source x="1307.4738019182" y="1060.27987125479" />
				<position_destination x="1305.61352816716" y="1108.33240728083" />
			</editor_data>
			<guard>
				return SUCCESS;
			</guard>
			<event />
